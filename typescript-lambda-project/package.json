{
  "name": "typescript-lambda-project",
  "version": "1.0.0",
  "description": "TypeScript Lambda with CI/CD deployment",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "deploy": "cdk deploy",
    "synth": "cdk synth",
    "security:audit": "npm audit --audit-level=high",
    "lint": "eslint src/ --ext .ts"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "aws-cdk-lib": "^2.210.0",
    "babel-jest": "^29.0.0",
    "constructs": "^10.0.0",
    "jest": "^29.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7"
  }
}
